

/**
 *
 * @author MANU
 */
public class Paciente {
    
    String nombre, Dni;
    int edad;
    char sexo;
    double peso, altura;

    public Paciente(String nombre, String Dni, int edad, char sexo, double peso, double altura){
    
       this.nombre=nombre;
       this.Dni=Dni;
       this.edad=edad;
       this.sexo=sexo;
       this.peso=peso;
       this.altura=altura;
    }
    
    
    
}
